    "@types/jest": "^19.2.4",
    "angular": "1.6.4",
    "angular-mocks": "1.5.9",
    "jest": "^20.0.4",
    "ng-annotate-loader": "^0.6.1",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.1.0",
    "typescript": "2.2.1",
    "webpack": "^2.6.1",
    "webpack-build-notifier": "^0.1.13"
  }
